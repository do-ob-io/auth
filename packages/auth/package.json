{
  "name": "@do-ob/auth",
  "version": "0.0.0",
  "description": "Authentication (authn) and authorization (authz) library",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "keywords": [
    "auth",
    "typescript",
    "library",
    "esm",
    "authn",
    "authz",
    "webauthn",
    "authentication",
    "authorization"
  ],
  "author": "Eric Crowell <eric@do-ob.io> (https://do-ob.io)",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run build:dist && pnpm run build:types",
    "build:dist": "vite build --mode production",
    "build:types": "tsc -p tsconfig.build.json"
  }
}
