{
  "name": "@do-ob/auth",
  "version": "0.0.0",
  "description": "Authentication (authn) and authorization (authz) library",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./types/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    },
    "./index": null
  },
  "files": [
    "dist",
    "types"
  ],
  "keywords": [
    "auth",
    "typescript",
    "library",
    "esm",
    "authn",
    "authz",
    "webauthn",
    "authentication",
    "authorization"
  ],
  "author": "Eric Crowell <eric@do-ob.io> (https://do-ob.io)",
  "license": "MIT",
  "scripts": {
    "test": "vitest --run",
    "test:browser": "vitest --run --browser.name=chrome --browser.headless",
    "build": "vite build --mode production"
  },
  "dependencies": {
    "@do-ob/auth": "workspace:*",
    "@do-ob/crypto": "workspace:*"
  }
}
